<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sign In</title>
</head>
<body>
    <h2>Sign In</h2>
    <h3>Email ID</h3>
    <input type = "email" id = "email">
    <h3>Password</h3>
    <input type = "password" id = "password">
    
    <button onclick = "signin()">Sign in</button>
<script>
    function signin(){
        let email = document.getElementById("email").value;
        let password = document.getElementById("password").value;

        fetch("/signin1", {method:"POST", headers:{"Content-Type": "application/json"},body: JSON.stringify({email, password})}).then(function(res){
            console.log(email,password);
            return res.json();
        }).then(function(res2){
            console.log(res2);
        })

    }
</script>
<script></script>
</body>
</html>